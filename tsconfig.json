{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": ".",
    "moduleResolution": "node"
  },
  "include": ["problems/**/*"],
  "exclude": ["node_modules", "dist"]
} 